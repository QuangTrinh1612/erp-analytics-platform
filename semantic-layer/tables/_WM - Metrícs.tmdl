	/// VAR _max_date = MAX('DIM - DATE'[Date])
	/// var _measure = 
	/// SWITCH(
	///         SELECTEDVALUE('PARAMETER - EXCLUDE REVERSE DOCUMENT'[EXCLUDE_REVERSE],"")
	///         ,"",
	///         CALCULATE(
	///             SUMX('AGG - MATERIAL LEDGER V PRICE',[RECEIPT_AMT] + [ISSUE_AMT]),
	///             'DIM - DATE'[Date] < _min_date
	///         )
	///         ,"X",
	///         CALCULATE(
	///             SUMX('AGG - MATERIAL LEDGER V PRICE',[RECEIPT_AMT] + [ISSUE_AMT] - [REVERSE_AMT]),
	///             'DIM - DATE'[Date] < _min_date
	///         )
	///         +
	///         CALCULATE(
	///             SUMX('AGG - MATERIAL LEDGER V PRICE',[REVERSE_REMAIN_AMT]),
	///             'DIM - DATE'[Date] >= _min_date
	///         )
	///     )
	/// RETURN
	/// IF(ROUND(_MEASURE,3)=0,BLANK(),_MEASURE)
	measure 'OPENING AMT (V PRICE)' = ```
			
			VAR _min_date = MIN('DIM - DATE'[Date])
			VAR _max_date = MAX('DIM - DATE'[Date])
			var _measure = 
			SWITCH(
			        SELECTEDVALUE('PARAMETER - EXCLUDE REVERSE DOCUMENT'[EXCLUDE_REVERSE],"")
			        ,"",
			        CALCULATE(
			            SUMX('AGG - MATERIAL LEDGER V PRICE',[RECEIPT_AMT] + [ISSUE_AMT]),
			            'DIM - DATE'[Date] < _min_date
			        )
			        ,"X",
			        CALCULATE(
			            SUMX('AGG - MATERIAL LEDGER V PRICE',[RECEIPT_AMT] + [ISSUE_AMT] - [REVERSE_AMT]),
			            'DIM - DATE'[Date] < _min_date
			        )
			        +
			        CALCULATE(
			            SUMX('AGG - MATERIAL LEDGER V PRICE',[REVERSE_REMAIN_AMT]),
			            'DIM - DATE'[Date] >= _min_date
			        )
			    )
			RETURN
			IF(ROUND(_MEASURE,3)=0,BLANK(),_MEASURE)
