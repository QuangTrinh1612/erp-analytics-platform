	/// Formula: 
	/// VAR _MIN_DATE =
	///     CALCULATE(
	///         MIN('DIM - DATE'[Date]),
	///         ALLSELECTED('DIM - DATE'[Date])
	///     )
	///  
	/// VAR _MAX_DATE =
	///     CALCULATE(
	///         MAX('DIM - DATE'[Date]),
	///         ALLSELECTED('DIM - DATE'[Date])
	///     )
	/// RETURN
	///     SUMX(
	///         SUMMARIZE(
	///             FILTER(
	///                 ALL('AGG - MATERIAL LEDGER SUPPLY'),
	///                 'AGG - MATERIAL LEDGER SUPPLY'[POSTING_DATE] <= _MAX_DATE
	///             ),
	///             [MATERIAL_CODE],
	///             [STORAGE_LOC],
	///     
	///             "ISSUE_AMT",
	///                 CALCULATE(
	///                     SUM('AGG - MATERIAL LEDGER SUPPLY'[ISSUE_AMT]),
	///                     'DIM - DATE'[Date] <= _MAX_DATE
	///                     && 'DIM - DATE'[Date] >= _MIN_DATE
	///                 ),
	///             "RECEIPT_AMT",
	///                 CALCULATE(
	///                     SUM('AGG - MATERIAL LEDGER SUPPLY'[RECEIPT_AMT]),
	///                     'DIM - DATE'[Date] <= _MAX_DATE
	///                     && 'DIM - DATE'[Date] >= _MIN_DATE,
	///                     NOT(ISBLANK('AGG - MATERIAL LEDGER SUPPLY'[PO_DOC]))
	///                 ),
	///             "CLOSING_STOCK",
	///                 CALCULATE(
	///                     SUMX('AGG - MATERIAL LEDGER V PRICE',[RECEIPT_AMT] + [ISSUE_AMT]),
	///                     'DIM - DATE'[Date] <= _MAX_DATE
	///                 )
	///         ),
	///         IF(COALESCE([ISSUE_AMT],0) = 0 && COALESCE([RECEIPT_AMT],0) <> 0 ,
	///             [CLOSING AMT (V PRICE)],
	///             0
	///         )
	///     )
	measure 'Tồn kho nhập không xuất' = ```
			
			VAR _MIN_DATE =
			    CALCULATE(
			        MIN('DIM - DATE'[Date]),
			        ALLSELECTED('DIM - DATE'[Date])
			    )
			 
			VAR _MAX_DATE =
			    CALCULATE(
			        MAX('DIM - DATE'[Date]),
			        ALLSELECTED('DIM - DATE'[Date])
			    )
			RETURN
			    SUMX(
			        SUMMARIZE(
			            FILTER(
			                ALL('AGG - MATERIAL LEDGER SUPPLY'),
			                'AGG - MATERIAL LEDGER SUPPLY'[POSTING_DATE] <= _MAX_DATE
			            ),
			            [MATERIAL_CODE],
			            [STORAGE_LOC],
			    
			            "ISSUE_AMT",
			                CALCULATE(
			                    SUM('AGG - MATERIAL LEDGER SUPPLY'[ISSUE_AMT]),
			                    'DIM - DATE'[Date] <= _MAX_DATE
			                    && 'DIM - DATE'[Date] >= _MIN_DATE
			                ),
			            "RECEIPT_AMT",
			                CALCULATE(
			                    SUM('AGG - MATERIAL LEDGER SUPPLY'[RECEIPT_AMT]),
			                    'DIM - DATE'[Date] <= _MAX_DATE
			                    && 'DIM - DATE'[Date] >= _MIN_DATE,
			                    NOT(ISBLANK('AGG - MATERIAL LEDGER SUPPLY'[PO_DOC]))
			                ),
			            "CLOSING_STOCK",
			                CALCULATE(
			                    SUMX('AGG - MATERIAL LEDGER V PRICE',[RECEIPT_AMT] + [ISSUE_AMT]),
			                    'DIM - DATE'[Date] <= _MAX_DATE
			                )
			        ),
			        IF(COALESCE([ISSUE_AMT],0) = 0 && COALESCE([RECEIPT_AMT],0) <> 0 ,
			            [CLOSING AMT (V PRICE)],
			            0
			        )
			    )
			```
