	column 'Nhóm Material' =
			SWITCH(TRUE(),
			IF(CONTAINSSTRING('DIM - MATERIAL PP'[MATERIAL_SHORT_DESCRIPTION] , "SPDD"),TRUE()), "SPDD",
			IF(CONTAINSSTRING('DIM - MATERIAL PP'[MATERIAL_SHORT_DESCRIPTION] , "BTP-NPK"),TRUE()), "BTP-NPK",
			 [MATL_TYPE] = "Z50" && IF(CONTAINSSTRING('DIM - MATERIAL PP'[MATERIAL_SHORT_DESCRIPTION] , "Bio"),TRUE()), "Ure Chức Năng",
			 [MATL_TYPE] = "Z50" && IF(CONTAINSSTRING('DIM - MATERIAL PP'[MATERIAL_SHORT_DESCRIPTION] , "TM"),TRUE()), "Ure Thương Mại",
			  [MATL_TYPE] = "Z50" && IF(CONTAINSSTRING('DIM - MATERIAL PP'[MATERIAL_SHORT_DESCRIPTION] , "thương mại"),TRUE()), "Ure Thương Mại",
			  [MATL_TYPE] = "Z50" && IF(CONTAINSSTRING('DIM - MATERIAL PP'[MATERIAL_SHORT_DESCRIPTION] , "N46"),TRUE()), "Ure Chức Năng",
			   [MATL_TYPE] = "Z50" && IF(CONTAINSSTRING('DIM - MATERIAL PP'[MATERIAL_SHORT_DESCRIPTION] , "Phế Phẩm"),TRUE()), "Ure Phế Phẩm",
			   [MATL_TYPE] = "Z51" && IF(CONTAINSSTRING('DIM - MATERIAL PP'[MATERIAL_SHORT_DESCRIPTION] , "NPK"),TRUE()), "NPK",
			   [MATL_TYPE] = "Z40" , "BTP Ure",
			  IF(ISBLANK( LOOKUPVALUE('DIM - MATERIALS NPK'[MATERIAL_CODE], 'DIM - MATERIALS NPK'[MATERIAL_CODE], 'DIM - MATERIAL PP'[MATERIAL_CODE])), 'DIM - MATERIAL PP'[SPECIALIZE],LOOKUPVALUE('DIM - MATERIALS NPK'[NOTE_1], 'DIM - MATERIALS NPK'[MATERIAL_CODE], 'DIM - MATERIAL PP'[MATERIAL_CODE]) ))
		dataType: string
		lineageTag: 0e02b60c-a06f-4972-ad9b-e854a9f6c34d
		summarizeBy: none
		isDataTypeInferred

		annotation SummarizationSetBy = Automatic

