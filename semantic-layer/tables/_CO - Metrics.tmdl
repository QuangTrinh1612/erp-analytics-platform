	measure 'Sản lượng tiêu thụ' = [Consumption Qty (Plant)]
		formatString: #,##0
		lineageTag: ebad7fa1-7bca-40b0-acc7-6072db0f6838

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Sản lượng SX (ZCO12A)' =
			
			/*
			IF(
			    MAX('FACT - CO SPLIT ITEM'[PROCESS_CAT]) = "BF",
			    CALCULATE(
			        SUMX(
			            'FACT - CO SPLIT ITEM',
			            [LOCAL_AMT]
			        ),
			        FILTER(
			            'FACT - CO SPLIT ITEM',
			            [COST_COMPONENT] = "QUANTITY"
			        )
			    )
			)
			*/
			SUMX(
			    SUMMARIZE(
			        FILTER(
			            'FACT - CO SPLIT ITEM',
			            [COST_COMPONENT] = "QUANTITY"
			        ),
			        'DIM - MATERIAL'[MATERIAL_CODE],
			        'FACT - CO SPLIT ITEM'[COST_COMPONENT_STRUCTURE],
			        "_process_cat", MAX('FACT - CO SPLIT ITEM'[PROCESS_CAT]),
			        "_amt", SUM('FACT - CO SPLIT ITEM'[LOCAL_AMT])
			    ),
			    IF([_process_cat] = "BF" || [COST_COMPONENT_STRUCTURE] = "T4", [_amt])
			)
		formatString: #,0.000
		lineageTag: 76b01beb-66e1-4a5f-b0ff-7197c3b4f6ad

	measure 'Sản lượng SX' =
			
			VAR _production_receipt_qty =
			    SUMX(
			        'FACT - INVENTORY BALANCE PLANT',
			        [PRODUCTION_RECEIPT_QTY]
			    )
			VAR _receipt_qty =
			    SUMX(
			        'FACT - INVENTORY BALANCE PLANT',
			        [RECEIPT_QTY]
			    )
			RETURN
			    SWITCH(
			        TRUE(),
			        _production_receipt_qty <> 0, _production_receipt_qty,
			        _receipt_qty <> 0, _receipt_qty,
			        BLANK()
			    )
		formatString: #,0.000
		lineageTag: 14da1235-9428-4bc2-b9c9-d787d63366db

	measure 'Lợi nhuận' = /*CALCULATE([Số liệu P&L kỳ này], 'DIM - REPORTING LINE IS'[ITEM_NO] = "20")*/ [Doanh thu] - [Giá vốn]
		formatString: #,##0
