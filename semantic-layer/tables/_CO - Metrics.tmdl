			            'FACT - CO SPLIT ITEM'[MATERIAL_CODE],
			            'FACT - CO SPLIT ITEM'[COST_COMPONENT_STRUCTURE],
			            "_amt",
			                SUM('FACT - CO SPLIT ITEM'[LOCAL_AMT])
			        ),
			        IF(
			            [COST_COMPONENT_STRUCTURE] = "T4"
			            || _process_cat = "BF",
			            [_amt]
