	measure 'TH Chi phí bán hàng (ZCO05)' =
			
			VAR _CPBH_NPK =
			CALCULATE(
			    SUMX(
			        'FACT - GL ACCOUNT BALANCE OFFSET',
			        [DEBIT_AMT] + [CREDIT_AMT]
			    ),
			    FILTER(
			        'FACT - GL ACCOUNT BALANCE OFFSET',
			        'FACT - GL ACCOUNT BALANCE OFFSET'[GL_ACCOUNT_NO]
			                IN {"6417000203",
			                    "6417300301",
			                    "6417300302",
			                    "6417300303",
			                    "6417300304",
			                    "6418000109",
			                    "6418100307"
			                }
			    )
			)
			VAR _CPBH_TRU_NPK =
			CALCULATE(
			    SUMX(
			        'FACT - GL ACCOUNT BALANCE OFFSET',
			        [DEBIT_AMT] + [CREDIT_AMT]
			    ),
			    FILTER(
			        'FACT - GL ACCOUNT BALANCE OFFSET',
			        LEFT('FACT - GL ACCOUNT BALANCE OFFSET'[GL_ACCOUNT_NO],3) = "641"
			        && RIGHT('FACT - GL ACCOUNT BALANCE OFFSET'[GL_ACCOUNT_NO],6) <> "999999"
			        && NOT(
			            'FACT - GL ACCOUNT BALANCE OFFSET'[GL_ACCOUNT_NO]
			                IN {"6417000203",
			                    "6417300301",
			                    "6417300302",
			                    "6417300303",
			                    "6417300304",
			                    "6418000109",
			                    "6418100307"
			                    }
			        )
			    )
			)
			VAR _SL_URE =
			CALCULATE(
			    SUM('FACT - COPA LINE ITEM'[SALE_QUANTITY]),
			    'FACT - COPA LINE ITEM'[VERSION] = ""
			    && 'FACT - COPA LINE ITEM'[MATERIAL_GROUP] = "Z500001"
			    && 'FACT - COPA LINE ITEM'[MATERIAL_CODE] <> "0050499999"
			)
			VAR _SL_NPK =
			CALCULATE(
			    SUM('FACT - COPA LINE ITEM'[SALE_QUANTITY]),
			    'FACT - COPA LINE ITEM'[VERSION] = ""
			    && 'FACT - COPA LINE ITEM'[MATERIAL_GROUP] = "Z510001"
			    && 'FACT - COPA LINE ITEM'[MATERIAL_CODE] <> "0050499999"
			)
			RETURN
			SWITCH(
			    SELECTEDVALUE('Parameter: Báo cáo Ure & NPK'[Loại báo cáo],""),
			    "Ure",
			        CALCULATE(
			            SUMX(
			                'FACT - GL ACCOUNT BALANCE OFFSET',
			                [DEBIT_AMT] + [CREDIT_AMT]
			            ),
			            FILTER(
			                'FACT - GL ACCOUNT BALANCE OFFSET',
			                LEFT('FACT - GL ACCOUNT BALANCE OFFSET'[GL_ACCOUNT_NO],3) = "641"
			                && RIGHT('FACT - GL ACCOUNT BALANCE OFFSET'[GL_ACCOUNT_NO],6) <> "999999"
			                && [COST_CENTRE] <> "1A10"
			                && NOT(
			                    'FACT - GL ACCOUNT BALANCE OFFSET'[GL_ACCOUNT_NO]
			                        IN {"6417000203",
			                            "6417300301",
			                            "6417300302",
			                            "6417300303",
			                            "6417300304"
			                            -- "6418000109",
			                            -- "6418100307"
			                            }
			                )
			            )
			        ),
			    "NPK",
			        _CPBH_NPK + DIVIDE(_CPBH_TRU_NPK * _SL_NPK, _SL_URE + _SL_NPK)
			)
		formatString: #,##0
