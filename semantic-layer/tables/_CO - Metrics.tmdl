	measure 'Chi phí tài chính / bao (2000)' =
			
			VAR _cpbh =
			CALCULATE(
			    SUM('FACT - INCOME STATEMENT'[LOCAL_AMT]),
			    FILTER(
			        'FACT - INCOME STATEMENT',
			        [IS_ITEM] IN {"23"}
			    )
			)
			VAR _sl = [Sản lượng tiêu thụ]
			RETURN
			    DIVIDE(_cpbh * [Hệ số quy đổi (2000)], _sl)
