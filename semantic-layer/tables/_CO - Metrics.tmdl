	measure 'Hệ số quy đổi (2000)' =
			
			DIVIDE(
			    SUMX(
			        'FACT - INVENTORY BALANCE PLANT',
			        SWITCH(
			            RELATED('DIM - MATERIAL'[MATERIAL_CODE]),
			            "0026000000", 'FACT - INVENTORY BALANCE PLANT'[CONSUMPTION_QTY] * 100,
			            "0026000001", 'FACT - INVENTORY BALANCE PLANT'[CONSUMPTION_QTY] * 200
			        )
			    ),
			    CALCULATE(
			        SUMX(
			            'FACT - INVENTORY BALANCE PLANT',
			            SWITCH(
			                RELATED('DIM - MATERIAL'[MATERIAL_CODE]),
			                "0026000000", 'FACT - INVENTORY BALANCE PLANT'[CONSUMPTION_QTY] * 100,
			                "0026000001", 'FACT - INVENTORY BALANCE PLANT'[CONSUMPTION_QTY] * 200
			            )
			        ),
			        ALL('DIM - MATERIAL')
			    )
			)
		formatString: #,##0
		lineageTag: 958bd21c-c101-4e9a-b4dd-ab7851cc6da8

		annotation PBI_FormatHint = {"isGeneralNumber":true}

	measure 'Giá vốn' = CALCULATE([Số liệu P&L kỳ này], 'DIM - REPORTING LINE IS'[ITEM_NO]= "11", 'DIM - GL ACCOUNT'[GL_ACC_NO]<> "6322020590",USERELATIONSHIP('DIM - MATERIAL'[MATERIAL_CODE], 'FACT - INCOME STATEMENT'[MATERIAL_CODE]))
