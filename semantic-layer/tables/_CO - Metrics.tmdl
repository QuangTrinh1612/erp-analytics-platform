	measure 'Chi phí kế hoạch (ZCO14 - DMQL)' = ```
			
			VAR _CNT = DISTINCTCOUNT('FACT - COST PLANNING'[COST_CENTER])
			RETURN  
			SUMX(
			    SUMMARIZE(   
			            'FACT - COST PLANNING',
			            [COST_CENTER],
			            [COST_ELEMENT],
			            "CNT",
			                _CNT,
			            "AMT",
			                CALCULATE(
			                    SUM('FACT - COST PLANNING'[TRANSACTION_AMT]),
			                    ALL('DIM - DATE'[Month])
			                )
			    ),
			    SWITCH(
			        TRUE(),
			        [CNT] > 1 && [COST_CENTER] = "1A10" && LEFT([COST_ELEMENT],3) = "621",[AMT],
			        [CNT] = 1 && LEFT([COST_ELEMENT],3) = "621", [AMT],
			        LEFT('FACT - COST PLANNING'[COST_ELEMENT], 3) <> "621", [AMT]
			    )
			)
